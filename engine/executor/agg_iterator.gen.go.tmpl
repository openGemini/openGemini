func New{{.Name}}Col{{.Name}}SampleIterator(sampleNum int,
	isSingleCall bool, inOrdinal, outOrdinal int, auxProcessor []*AuxProcessor, rowDataType hybridqp.RowDataType,
) *{{.Name}}Col{{.Name}}SampleIterator {
